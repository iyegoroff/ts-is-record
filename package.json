{
  "name": "ts-is-record",
  "version": "3.0.3",
  "description": "Typescript utilities",
  "sideEffects": false,
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "react-native": "./dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "preversion": "npm run lint && npm run build",
    "postversion": "git push && git push --tags",
    "build": "rm -rf dist && rollup -c rollup.config.ts --configPlugin typescript && rm -rf tmp",
    "lint": "tslint -p . && tslint test/*",
    "watch": "tsc -p . -w"
  },
  "keywords": [
    "typescript",
    "assert"
  ],
  "files": [
    "dist",
    "src"
  ],
  "author": "iyegoroff <iegoroff@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iyegoroff/ts-is-record/issues"
  },
  "homepage": "https://github.com/iyegoroff/ts-is-record#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iyegoroff/ts-is-record.git"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.2",
    "rollup": "^2.73.0",
    "rollup-plugin-dts": "^4.2.1",
    "tslint": "^6.1.3",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "ts-tiny-invariant": "^2.0.3"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "none",
    "singleQuote": true
  }
}
