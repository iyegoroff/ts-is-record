{
  "name": "ts-is-record",
  "version": "3.0.1",
  "description": "Typescript utilities",
  "sideEffects": false,
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/dts/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "type": "module",
  "scripts": {
    "preversion": "npm run lint && npm run build",
    "postversion": "git push && git push --tags",
    "build:dts": "tsc -p configs/tsconfig.dts.json",
    "build:esm": "tsc -p configs/tsconfig.esm.json",
    "build:cjs": "build-for-cjs configs/tsconfig.cjs.json",
    "build": "rm -rf dist && mkdir dist && npm run build:dts && npm run build:esm && npm run build:cjs",
    "lint": "tslint -p . && tslint test/*",
    "watch": "tsc -p . -w"
  },
  "keywords": [
    "typescript",
    "assert"
  ],
  "files": [
    "dist",
    "src"
  ],
  "author": "iyegoroff <iegoroff@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iyegoroff/ts-is-record/issues"
  },
  "homepage": "https://github.com/iyegoroff/ts-is-record#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iyegoroff/ts-is-record.git"
  },
  "devDependencies": {
    "build-for-cjs": "^0.0.8",
    "tslint": "^6.1.3",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "ts-tiny-invariant": "^2.0.1"
  }
}
